{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "experimentalDecorators": true,
    "target": "ES2019",
    "module": "CommonJS",
    "lib": ["ES2019", "DOM", "DOM.Iterable"],
    "noImplicitAny": false,
    "sourceMap": false,
    "allowJs": true,
    "types": ["webpack-env"]
  },
  "include": ["ExtensionUtils.ts", "**/*.ts", "**/*.vue", ".eslintrc.js"],
  "exclude": ["**/tsconfig*.json", "**/node_modules", "**/build"]
}
